{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.OpenTelemetry" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "OpenTelemetry",
        "Args": {
          "Endpoint": "http://grafana:4317",
          "Protocol": "Grpc",
          "ResourceAttributes": {
            "service.name": "api-admin",
            "deployment.environment": "development"
          }
        }
      }
    ],
    "Enrich": [ "WithMachineName", "WithProcessId", "WithThreadId" ]
  },
  "AllowedHosts": "*",
  "DatabaseOptions": {
    "ConnectionString": "Server=.;Database=PolyQubeAdmin;Trusted_Connection=True;TrustServerCertificate=True;MultipleActiveResultSets=True"
  },
  "AuthValidationOptions": {
    "Authority": "https://localhost:7210",
    "AccessTokenValidationEndpoint": "/api/v1/auth/validate-access-token"
  },
  "CorsOptions": {
    "AllowedOrigins": [
      "https://localhost:7254"
    ]
  },
  "TelemetryOptions": {
    "TempoEndpoint": "http://localhost:9010",
    "MetricsEndpoint": "http://localhost:9010"
  },
  "MongoDbOptions": {
    "ConnectionString": "mongodb://user:pass@localhost:27017/"
  },
  "RabbitMqOptions": {
    "Host": "localhost",
    "Username": "guest",
    "Password": "guest"
  },
  "EmailSenderOptions": {
    "SystemEmail": "tierra.jenkins68@ethereal.email",
    "SmtpHost": "smtp.ethereal.email",
    "SmtpPort": 587,
    "EmailUsername": "...",
    "EmailPassword": "..."
  },
  "FrontendLinksOptions": {
    "DeleteUserLink": "http://localhost/smthSmth/"
  }
}
