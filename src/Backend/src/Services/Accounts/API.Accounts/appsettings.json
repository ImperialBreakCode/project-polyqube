{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.OpenTelemetry" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "OpenTelemetry",
        "Args": {
          "Endpoint": "http://grafana:4317",
          "Protocol": "Grpc",
          "ResourceAttributes": {
            "service.name": "api-accounts",
            "deployment.environment": "development"
          }
        }
      }
    ],
    "Enrich": [ "WithMachineName", "WithProcessId", "WithThreadId" ]
  },
  "AllowedHosts": "*",
  "DatabaseOptions": {
    "ConnectionString": "Server=.;Database=PolyQubeAccounts;Trusted_Connection=True;TrustServerCertificate=True;MultipleActiveResultSets=True"
  },
  "SagasDbOptions": {
    "ConnectionString": "Server=.;Database=PolyQubeAccountSagas;Trusted_Connection=True;TrustServerCertificate=True;MultipleActiveResultSets=True"
  },
  "AuthTokenOptions": {
    "Issuer": "testIssuer",
    "Audience": "testAudiance",
    "SecretKey": "756254D9744B3775ABC629393A5C1"
  },
  "AuthValidationOptions": {
    "Authority": "https://localhost:7210",
    "AccessTokenValidationEndpoint": "/api/v1/auth/validate-access-token"
  },
  "CorsOptions": {
    "AllowedOrigins": [
      "https://localhost:7254"
    ]
  },
  "TelemetryOptions": {
    "TempoEndpoint": "http://localhost:9010",
    "MetricsEndpoint": "http://localhost:9010"
  },
  "RedisOptions": {
    "RedisHost": "localhost:6379"
  },
  "RabbitMqOptions": {
    "Host": "localhost",
    "Username": "guest",
    "Password": "guest"
  },
  "MongoDbOptions": {
    "ConnectionString": "mongodb://user:pass@localhost:27017/"
  },
  "InitialSuperAdminOptions": {
    "Username": "root",
    "Password": "root",
    "Email": "tierra.jenkins68@ethereal.email"
  }
}
